{
  "name": "@codebuff/internal",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./db": {
      "bun": "./src/db/index.ts",
      "import": "./src/db/index.ts",
      "types": "./src/db/index.ts",
      "default": "./src/db/index.ts"
    },
    "./loops": {
      "bun": "./src/loops/index.ts",
      "import": "./src/loops/index.ts",
      "types": "./src/loops/index.ts",
      "default": "./src/loops/index.ts"
    },
    "./openrouter-ai-sdk": {
      "bun": "./src/openrouter-ai-sdk/index.ts",
      "import": "./src/openrouter-ai-sdk/index.ts",
      "types": "./src/openrouter-ai-sdk/index.ts",
      "default": "./src/openrouter-ai-sdk/index.ts"
    },
    "./*": {
      "bun": "./src/*.ts",
      "import": "./src/*.ts",
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit -p .",
    "test": "bun test",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "drizzle-kit push --config=./src/db/drizzle.config.ts",
    "db:start": "docker compose -f ./src/db/docker-compose.yml up --wait && bun run db:generate && (timeout 1 || sleep 1) && bun run db:migrate",
    "db:studio": "drizzle-kit studio --config=./src/db/drizzle.config.ts"
  },
  "sideEffects": false,
  "engines": {
    "bun": "^1.3.0"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "3.0.7",
    "@codebuff/common": "workspace:*",
    "drizzle-orm": "*",
    "loops": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "22",
    "@types/bun": "^1.3.0"
  }
}
